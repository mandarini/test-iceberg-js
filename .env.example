# ===========================================
# Configuration for real-test.ts (iceberg-js)
# ===========================================

# Secret key - DO NOT commit the real value
SUPABASE_TOKEN=your_service_role_key_here

# Name of your Iceberg warehouse (typically "warehouse")
SUPABASE_WAREHOUSE=warehouse

# Iceberg REST Catalog endpoint
# Format: https://{project-ref}.storage.supabase.{region}/storage/v1/iceberg
SUPABASE_CATALOG_URI=https://your-project-ref.storage.supabase.co/storage/v1/iceberg

# ===========================================
# Configuration for real-test-supabase-js.ts
# ===========================================

# Supabase project URL
# Format: https://{project-ref}.supabase.co
SUPABASE_URL=https://your-project-ref.supabase.co

# Secret key (required for analytics bucket operations)
# Get from: Settings → API → Project API keys → service_role (secret)
# WARNING: Keep this secret! Never expose in client-side code
SUPABASE_SECRET_KEY=your_service_role_key_here

# Analytics bucket name (the warehouse name)
ANALYTICS_BUCKET_NAME=warehouse

# Optional: AWS credentials for direct S3 access
# Only needed if you want to access storage directly via S3 API
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Optional: S3 endpoint URL
# Format: https://{project-ref}.storage.supabase.{region}/storage/v1/s3
S3_ENDPOINT=https://your-project-ref.storage.supabase.co/storage/v1/s3
